        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Statistics_of_valedictorians"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="btnBrowse" Content="..." HorizontalAlignment="Left" Margin="420,71,0,0" VerticalAlignment="Top" Width="47" Click="btnBrowse_Click"/>
        <TextBox x:Name="txtPath" HorizontalAlignment="Left" Margin="230,40,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="143" Height="46"/>
        <ComboBox x:Name="cbYear" HorizontalAlignment="Left" Margin="230,117,0,0" VerticalAlignment="Top" Width="143"/>
        <DataGrid x:Name="dgScores" Margin="0,161,0,0"/>
    </Grid>
</Window>
